                    Name = "background",
                    BackgroundTransparency = 1,
                    MidImage = "rbxassetid://103468666327206",
                    TopImage = "rbxassetid://103468666327206",
                    BottomImage = "rbxassetid://103468666327206",
                    Position = dim2(0, 1, 0, 1),
                    Size = dim2(1, -2, 1, 1),
